    "@storybook/blocks": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/test-runner": "^0.13.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/compress.js": "^1.1.1",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
