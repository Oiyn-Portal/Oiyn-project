    "antd": "^5.9.0",
    "classnames": "^2.3.2",
    "compress.js": "^1.2.2",
    "dayjs": "^1.11.9",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "formik": "^2.4.4",
    "history": "^5.3.0",
    "husky": "^8.0.3",
    "js-cookie": "^3.0.5",
    "po-extract": "^1.1.2",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
